# Cell 1
import numpy as np
import matplotlib.pyplot as plt

def swarm_growth(n0, r, t):
    """n(t) = n0 × (1 + r)^t"""
    return n0 * (1 + r)**t

t_years = np.arange(0, 11)          # 0–10 years
n_start = 4000                      # initial modules
r_annual = 0.8                      # 80% annual growth (aggressive but illustrative)

n_t = swarm_growth(n_start, r_annual, t_years)

plt.figure(figsize=(9,5))
plt.semilogy(t_years, n_t, 'o-', label=f'Initial = {n_start}, growth rate = {r_annual*100:.0f}%/yr')
plt.xlabel('Years')
plt.ylabel('Number of swarm modules (log scale)')
plt.title('Exponential swarm replication curve')
plt.grid(True, which="both", ls="--")
plt.legend()
plt.show()

print(f"After {t_years[-1]} years: ~{n_t[-1]:,.0f} modules")
